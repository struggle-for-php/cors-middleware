{
    "name": "struggle-for-php/sfp-cors-middleware",
    "description": "PSR-7 CORS Middleware",
    "keywords": [
        "middleware",
        "cors"
    ],
    "homepage": "https://github.com/struggle-for-php/sfp-cors-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Mika Tuupola",
            "email": "tuupola@appelsiini.net",
            "homepage": "http://www.appelsiini.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1",
        "neomerx/cors-psr7": "^1.0",
        "http-interop/http-middleware": "^0.4.1"
    },
    "autoload": {
        "psr-4": {
            "Sfp\\CorsMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SfpTest\\CorsMiddleware\\": "tests"
        }
    },
    "require-dev": {
